<%= render 'users/info', user: @users %>
<%= render 'books/form', book: @book %>

<h1>Users</h1>
<table>
  <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
  </tr>
  <% @user.each do |user| %>
  <tr>
    <td><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: "jpg" %></td>
    <td><%= user.name %></td>
    <td><%= link_to "Show", user_path(user) %></td>
  </tr>
  <% end %>
</table>